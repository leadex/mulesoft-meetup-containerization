    example: !include ./examples/ExternalIdExample.json
  ExternalIdType:
    type: GlobalTypes.ExternalIdType
    example: !include ./examples/ExternalIdTypeExample.json
  UnitOfMeasure:
    type: GlobalTypes.UnitOfMeasure
    example: !include ./examples/UnitOfMeasureExample.json
  UserSession:
    type: GlobalTypes.UserSession
    example: !include ./examples/UserSessionExample.json
