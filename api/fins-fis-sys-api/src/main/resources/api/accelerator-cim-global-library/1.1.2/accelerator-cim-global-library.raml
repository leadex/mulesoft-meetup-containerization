#%RAML 1.0 Library
usage: Provides common types used across multiple CIM subject areas

uses:
  GlobalTypes: libraries/GlobalTypeLibrary.raml

types:
  # Exported GlobalTypes
  AuditInfo:
    type: GlobalTypes.AuditInfo
    example: !include ./examples/AuditInfoExample.json
  ExternalId:
